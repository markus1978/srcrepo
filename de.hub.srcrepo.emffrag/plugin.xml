<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
	<extension point="org.eclipse.emf.ecore.generated_package">
		<package
		        class="org.eclipse.gmt.modisco.java.emffrag.metadata.JavaPackage"
		        genModel="models/JavaModel.genmodel"
		        uri="http://www.eclipse.org/MoDisco/Java/0.2.incubation/java/emffrag"/>
		<package
		        class="de.hub.srcrepo.gitmodel.emffrag.metadata.GitModelPackage"
		        genModel="models/JavaModel.genmodel"
		        uri="http://gitmodel/1.0/emffrag"/>
		<package
		        class="de.hub.srcrepo.emffrag.extensions.ExtensionsPackage"
		        genModel="models/Extensions.genmodel"
		        uri="http://srcrepo/extensions/1.0">
		</package>
    </extension>
	
    <extension
	         id="application"
	         point="org.eclipse.core.runtime.applications">
		<application>
	         <run
	               class="de.hub.srcrepo.emffrag.EmfFragSrcRepoImport">
	         </run>
	    </application>
    </extension>
    <extension
          id="headless"
          point="org.eclipse.core.runtime.products">
       <product
             application="de.hub.srcrepo.emffrag.application"
             name="Import">
          <property
                name="appName"
                value="Import">
          </property>
       </product>
    </extension>
    <extension
          id="srcrepo"
          point="org.eclipse.core.runtime.products">
       <product
             application="de.hub.srcrepo.emffrag.application"
             name="srcrepo">
          <property
                name="appName"
                value="srcrepo">
          </property>
       </product>
    </extension>
	
</plugin>
